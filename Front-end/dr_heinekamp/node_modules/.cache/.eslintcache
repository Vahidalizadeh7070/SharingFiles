  <h5 className=\"card-title\">{props.fileName}</h5>\r\n                            <p>\r\n                                <FileIcon format={props.fileName} />\r\n                            </p>\r\n                            {props.showDeleteButton && <button type=\"button\" onClick={Delete} className=\"border border-1 btn btn-sm rounded-circle mx-1\">\r\n                                {isLoading && <div className=\"d-flex justify-content-center\">\r\n                                    <div className=\"spinner-border\" role=\"status\">\r\n                                        <span className=\"visually-hidden\">Loading...</span>\r\n                                    </div>\r\n                                </div>}\r\n                                {!isLoading && <XLg className=\"text-danger mb-1\" size={15} />}\r\n                            </button>\r\n                            }\r\n\r\n                            <button className=\"btn btn-primary btn-sm shadow rounded-circle mx-1\" type=\"button\" onClick={MySubFileDownload}>\r\n                                <Download className=\"mb-1\" />\r\n                            </button>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n           \r\n        </Fragment>\r\n    )\r\n}\r\nexport default SubFileItem;",{"ruleId":"137","severity":1,"message":"138","line":35,"column":19,"nodeType":"139","messageId":"140","endLine":35,"endColumn":23},{"ruleId":"137","severity":1,"message":"141","line":6,"column":12,"nodeType":"139","messageId":"140","endLine":6,"endColumn":17},{"ruleId":"137","severity":1,"message":"141","line":10,"column":12,"nodeType":"139","messageId":"140","endLine":10,"endColumn":17},{"ruleId":"137","severity":1,"message":"141","line":9,"column":12,"nodeType":"139","messageId":"140","endLine":9,"endColumn":17},{"ruleId":"137","severity":1,"message":"138","line":22,"column":19,"nodeType":"139","messageId":"140","endLine":22,"endColumn":23},"no-unuse